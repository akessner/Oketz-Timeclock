<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="okets.com.timeclock"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="14" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".TimeClockView"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="okets.com.timeclock.TimeClockView" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TimeClockActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TimeClockInsertNew"
            android:label="@string/app_name" >
              <intent-filter>
                <action android:name="okets.com.timeclock.TimeClockInsertNew" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        android:debuggable="true
    </application>

</manifest>